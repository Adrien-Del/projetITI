        <property name="MotDePasse" fieldname="MotDePasse" datatype="varchar" required="true" maxlength="100"/>
        <property name="Adresse" fieldname="Adresse" datatype="varchar" default="" maxlength="200"/>
        <property name="CodePostal" fieldname="CodePostal" datatype="int" default=""/>
        <property name="Ville" fieldname="Ville" datatype="varchar" default="" maxlength="50"/>
        <property name="NumTel" fieldname="NumTel" datatype="varchar" default="" maxlength="10"/>
